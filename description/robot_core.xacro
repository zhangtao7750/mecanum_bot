<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

      <material name="whitle">
        <color rgba="1 1 1 1"/>
      </material>  

      <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
      </material>  

      <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
      </material>  

      <material name="black">
        <color rgba="0 0 0 0"/>
      </material>  

      <link name="base_link">
        
      </link>

      <joint name="chsssis_joint" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="-0.1 0 0"/>
        
      </joint>

      <link name="chassis">
        <visual>
            <origin xyz="0.15 0 0.075"/>
            <geometry>
                <box size="0.3 0.3 0.15"/>
            </geometry>
            <material name="white">
            
        </visual>
        
      </link>

    
</robot>